{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "authorizenet/authorizenet": ">=1.9.3",
        "aws/aws-sdk-php": "^3.133",
        "bacon/bacon-qr-code": "^2.0",
        "binarystash/pdf-watermarker": "^1.1",
        "bkwld/cloner": "^3.5",
        "cviebrock/eloquent-sluggable": "^8.0.5",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.7",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "niklasravnsborg/laravel-pdf": "^4.0",
        "pragmarx/google2fa-laravel": "^1.3",
        "rap2hpoutre/fast-excel": "^3.0.0",
        "spatie/browsershot": "^3.60",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-database-mail-templates": "^3.2",
        "spatie/laravel-image-optimizer": "^1.5",
        "spatie/laravel-permission": "^3.2",
        "stripe/stripe-php": "^12.3",
        "twilio/sdk": "^8.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
